Algoritmo OperadoresRelacionales
	// Operadores Relacionales
	// Operador de Igualdad 
	a = 5 == 5
	Imprimir "Operador Igualdad: ", a
	//Operador diferente
	b = 5 <> 5
	Imprimir "Operador Distinto: ", b
	//Mayor que
	c = 6 > 5
	Imprimir "Operador mayor que: ", c
	//Mayor o igual que
	d = 6 >= 6
	Imprimir "Operador mayor o igual que: ", d
	//Menor que
	e = 7 < 7
	Imprimir "Operador menor que: ", e
	//Menor o igual que
	f = 7 <= 7
	Imprimir "Operador menor o igual que: ", f
FinAlgoritmo
